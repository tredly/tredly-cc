{
  "name": "tredly-cc",
  "version": "1.0.2",
  "description": "Tredly Command Center",
  "main": "server.js",
  "scripts": {
    "start": "forever start --minUptime 0 --spinSleepTime 1000 -a -c 'node --harmony' --uid tredly-cc server.js",
